{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "text-decoration-skip-ink",
          "-ms-filter"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}